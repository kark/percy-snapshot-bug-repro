{
  "name": "css-variables",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "________________________________codesandbox________________________________": "",
    "dev": "yarn build && yarn preview",
    "build": "vite build",
    "preview": "vite preview --host --port 3000",
    "start": "vite --host --port 3000",
    "test:e2e": "yarn jest -c jest.config.js",
    "vrt": "yarn percy exec -- yarn test:e2e",
    "vrt:local": "PERCY_TOKEN=$(grep 'PERCY_TOKEN.*' .env | sed 's/.*=//') PERCY_BRANCH=master yarn percy exec -- yarn test:e2e"
  },
  "devDependencies": {
    "@percy/cli": "1.6.1",
    "@percy/puppeteer": "2.0.2",
    "@vitejs/plugin-react": "^2.0.0",
    "jest": "27.4.5",
    "jest-puppeteer": "^6.0.3",
    "pptr-testing-library": "^0.7.0",
    "puppeteer": "15.4.0",
    "vite": "^3.0.0"
  }
}
